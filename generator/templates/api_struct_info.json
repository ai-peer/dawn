    {
        "file": "dawn/dawn.h",
        "defines": [],
        "structs": {
            {% for type in by_category["structure"] %}
                "{{as_cType(type.name)}}": [
                    {% if type.extensible %}
                        "nextInChain",
                    {% endif %}
                    {% for member in type.members %}
                        {% if loop.last %}
                            "{{as_varName(member.name)}}"
                        {% else %}
                            "{{as_varName(member.name)}}",
                        {% endif %}
                    {% endfor %}
                {% if loop.last %}
                    ]
                {% else %}
                    ],
                {% endif %}
            {% endfor %}
        }
    }
