#include <metal_stdlib>

using namespace metal;

constant static volatile bool tint_preserve_loop_var = false;
#define TINT_PRESERVE_LOOP() if (tint_preserve_loop_var) { break; } static_assert(true)

kernel void tint_symbol() {
  while(true) {
    TINT_PRESERVE_LOOP();
    if (true) {
      break;
    } else {
      return;
    }
  }
  int const x = 5;
  return;
}

