#include <metal_stdlib>

using namespace metal;

constant static volatile bool tint_loop_var = true;
#define TINT_LOOP if(tint_loop_var) 

kernel void tint_symbol() {
  int j = 0;
  TINT_LOOP for(int i = 0; (i < 2); i = as_type<int>((as_type<uint>(i) + as_type<uint>(2)))) {
    switch(i) {
      case 0: {
        switch(j) {
          case 0: {
            continue;
            break;
          }
          default: {
            break;
          }
        }
        break;
      }
      default: {
        break;
      }
    }
  }
  return;
}

