#include <metal_stdlib>

using namespace metal;

constant static volatile bool tint_preserve_loop_var = true;
#define TINT_PRESERVE_LOOP if (tint_preserve_loop_var)

void some_loop_body() {
}

void f() {
  int j = 0;
  for(int i = 0; ((i < 5) && (j < 10)); i = as_type<int>((as_type<uint>(i) + as_type<uint>(1)))) TINT_PRESERVE_LOOP {
    some_loop_body();
    j = as_type<int>((as_type<uint>(i) * as_type<uint>(30)));
  }
}

